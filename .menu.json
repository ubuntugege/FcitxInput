[
	{
		"id": "tools",
		"children":
		[
			{
				"id": "fcitxinput",
				"caption": "FcitxInput",
				"children":
				[
					{
						"caption": "Create Desktop Entry With Fcitx...",
						"command": "fcitxinput",
						"args":
						{
							"build_version": false
						}
					},
					{
						"caption": "Create Desktop Entry With Fcitx For Build Version...",
						"command": "fcitxinput",
						"args":
						{
							"build_version": true
						}
					}
				]
			}
		]

	},
	{
		"id": "preferences",
		"mnemonic": "n",
		"children":
		[
			{
				"caption": "Package Settings",
				"id": "package-settings",
				"mnemonic": "P",
				"children":
				[
					{
						"caption": "FcitxInput",
						"children":
						[
							{
								"caption": "README.md",
								"command": "open_file",
								"args": {
									"file": "${packages}/FcitxInput/README.md"
								}
							},
							{
								"caption": "-"
							},
							{
								"args": {
									"file": "${packages}/FcitxInput/FcitxInput.sublime-settings"
								},
								"caption": "Settings – Default",
								"command": "open_file"
							},
							{
								"args": {
									"file": "${packages}/User/FcitxInput.sublime-settings"
								},
								"caption": "Settings – User",
								"command": "open_file"
							}
						]
					}
				]
			}
		]
	}
]
